(ns html.HTMLSlotElement.ev
  "HTMLSlotElement Events."
  (:refer-clojure :exclude []))

(def slotchange
  "Event.

  The slotchange event is fired on an `html.HTMLSlotElement` instance
  element) when the node(s) contained in that slot change.

  See also: `https://developer.mozilla.org/en-US/docs/Web/API/HTMLSlotElement/slotchange_event`"

  "slotchange")

