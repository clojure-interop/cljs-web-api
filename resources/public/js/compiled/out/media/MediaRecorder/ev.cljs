(ns media.MediaRecorder.ev
  "MediaRecorder Events."
  (:refer-clojure :exclude []))

(def error
  "Event.

  The `media.MediaRecorder` interface's error event is fired when
  error occurs: for example because recording wasn't allowed or
  attempted using an unsupported codec.

  See also: `https://developer.mozilla.org/en-US/docs/Web/API/MediaRecorder/error_event`"

  "error")

