(ns media.MediaStreamTrack.ev
  "MediaStreamTrack Events."
  (:refer-clojure :exclude []))

(def mute
  "Event.

  The mute event is sent to a `media.MediaStreamTrack` when the
  source is temporarily unable to provide media data.

  See also: `https://developer.mozilla.org/en-US/docs/Web/API/MediaStreamTrack/mute_event`"

  "mute")

